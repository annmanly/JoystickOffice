<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:editor="UnityEditor.UIElements"
    xmlns:hr="Highrise.UI"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
>

<VisualElement class="character-outfit-editor">
    <VisualElement name="_top">
        <VisualElement name="_tabs">
            <VisualElement name="_tabAll" class="tab">
                <Label text="All"/>
            </VisualElement>
            <VisualElement name="_tabLiked" class="tab">
                <Label text="Liked"/>
            </VisualElement>
            <VisualElement name="_tabSelected" class="tab">
                <Label text="Selected"/>
            </VisualElement>
            <editor:ToolbarSearchField name="_search"/>
            <VisualElement name="_refresh"/>
        </VisualElement>
    </VisualElement>

    <VisualElement name="_content">
        <hr:UICollection editor="true">
            <hr:UICollectionSection name="_contentSection">
                <hr:UICollectionCells layout="grid" layout-spacing="8" fixed-height="true"/>
            </hr:UICollectionSection>
        </hr:UICollection>
    </VisualElement>

    <VisualElement name="_bottom">
        <VisualElement name="_groups">
        </VisualElement>
    </VisualElement>
</VisualElement>
</UXML>
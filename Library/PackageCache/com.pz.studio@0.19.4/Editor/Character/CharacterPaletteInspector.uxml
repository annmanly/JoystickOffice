<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:editor="UnityEditor.UIElements"
    xmlns:hr="Highrise.UI"
    xsi:noNamespaceSchemaLocation="../../../../UIElementsSchema/UIElements.xsd"
>

  <VisualElement class="character-palette-inspector">
    <Button name="_editButton" text="Open Palette Editor" />
    <editor:PropertyField name="_displayColumn" binding-path="_displayColumn" />
    <editor:PropertyField name="_primaryRowId" binding-path="_primaryRowId" />
    <editor:PropertyField name="_version" binding-path="_version" />
    <VisualElement name="_colorsContainer"/>
  </VisualElement>
</UXML>


.profiler {
    flex-grow: 1;
}

.profiler #_top {
    flex-direction: row;
}

.profiler #_sourceDropDown {
    flex: 1;
}

.profiler TwoPaneSplitView {
    flex-grow: 1;
    background-color: #25262e;
}


.profiler ._snapshotList {
    flex-grow: 1;
}

.profiler .treeContainer {
    flex-grow: 1;
}

.profiler .snapshot-entry {
    height: 38px;
    width: 100%;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    padding-left: 12px;
    padding-right: 12px;
}

.profiler .snapshot-entry .time-label {

}

.profiler .snapshot-entry .source-label {
    -unity-font-style: bold;
}

.profiler .snapshot-entry .size-label {

}

.profiler .snapshot-entry .snapshot-entry-left {
    margin-right: auto;
}
<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
>

  <VisualElement class="builtin-component-inspector">
    <VisualElement name="_header">
        <Image name="_assetIcon"/>
        <VisualElement name="_headerRight">
            <VisualElement name="_headerVertical">
                <Label name="_assetName" text="Name"/>
                <Label name="_assetDescription" text="Description"/>
            </VisualElement>
        </VisualElement>
    </VisualElement>

    <VisualElement name="_mainDisplay">
        <Button name="_addToSceneButton">
            <Label name="_addSceneLabel" text="Add To Scene As Prefab"/>
        </Button>
        <Label name="_orText" text="Or"/>
        <VisualElement name="_dragDisplay">
            <Image name="_dragAssetIcon"/>
            <Label name="_dragText" picking-mode="ignore"/>
        </VisualElement>  
    </VisualElement>  
  </VisualElement>
</UXML>
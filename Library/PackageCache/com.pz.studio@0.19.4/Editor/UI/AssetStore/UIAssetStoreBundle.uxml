<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:editor="UnityEditor.UIElements"
    xmlns:hr="Highrise.UI"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
>

    <VisualElement class="asset-store-bundle">
        <VisualElement name="_toolbar">
            <editor:ToolbarSearchField name="_search"/>
        </VisualElement>
        
        <hr:UICollection editor="true">
            <hr:UICollectionSection>
                <hr:UICollectionCells name="_prefabCells" layout="grid" layout-spacing="8" fixed-height="true"/>
            </hr:UICollectionSection>
        </hr:UICollection>
    </VisualElement>
</UXML>
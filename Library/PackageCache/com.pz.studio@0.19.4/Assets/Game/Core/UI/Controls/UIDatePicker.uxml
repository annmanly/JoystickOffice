<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIDatePicker class="date-picker">
    <VisualElement class="date-picker-container" name="_dayContainer">
      <hr:UILabel class="date-picker-info" key="day"/>
      <hr:UIScrollPicker name="_dayPicker"/>
    </VisualElement>
    <VisualElement class="date-picker-container" name="_monthContainer">
      <hr:UILabel class="date-picker-info" key="month"/>
      <hr:UIScrollPicker name="_monthPicker"/>
    </VisualElement>
    <VisualElement class="date-picker-container" name="_yearContainer">
      <hr:UILabel class="date-picker-info" key="year"/>
      <hr:UIScrollPicker name="_yearPicker"/>
    </VisualElement>
  </hr:UIDatePicker>

</UXML>
<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:hrui="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd">

  <hr:UIPopupPickerView name="popup-picker-view" class="popup-picker-view">
    <Label class="popup-picker-view__title" name="_label"/>
    <hr:UILabel class="popup-picker-view__subtitle" name="_subtitle"/>
    <hr:UIScrollView>
      <VisualElement name="_pickerChoices" class="flex-row flex-wrap">
      </VisualElement>
    </hr:UIScrollView>
    <hr:UIButton name="_applyButton" class="button-primary">
      <hr:UILabel key="apply"/>
    </hr:UIButton>
  </hr:UIPopupPickerView>

</UXML>
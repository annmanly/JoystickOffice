.action-sheet-item-preview-cell {
    height: 60px;
    align-items: center;
    margin-left: 12px;
    margin-right: 12px;
}

.action-sheet-item-preview-cell #_itemImage {
    border-radius: 8px;
    padding: 4px;
    width: 44px;
    height: 44px;
}

.action-sheet-item-preview-cell #_itemName {
    margin-left: 8px;
    -unity-font-definition: var(--font-bold);
    color: var(--color-fishbase-whitefish);
    white-space: normal;
    text-overflow: ellipsis;
    overflow: hidden;
}


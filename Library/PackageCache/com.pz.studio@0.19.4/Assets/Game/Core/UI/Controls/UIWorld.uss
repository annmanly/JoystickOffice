UIWorld {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
}

UIWorldElement {
    position: absolute;
    top: 0;
    left: 0;
}

.chat-bubble {
    border-radius: 8px;
    min-height: 22px;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 3px;
}

.chat-bubble-message {
    flex-wrap: wrap;
    font-size: 14px;
    max-width: 236px;
    padding-left: 8px;
    padding-right: 8px;
    padding-top: 4px;
    padding-bottom: 4px;
    white-space: normal;
    flex-shrink: 0;
}

.chat-bubble__message Label {
    color: var(--color-fishbase-eel);
    margin: 0;
    padding-left: 8px;
    padding-right: 8px;
    padding-top: 3px;
    padding-bottom: 3px;
}
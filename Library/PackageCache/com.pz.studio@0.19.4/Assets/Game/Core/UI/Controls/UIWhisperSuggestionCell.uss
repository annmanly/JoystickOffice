.whisper-suggestion-cell {
    flex-direction: row;
    margin-top: 4px;
    margin-bottom: 4px;
    height: 36px;
}

.whisper-suggestion-cell #_thumbnail {
    width: 36px;
    height: 36px;
}

.whisper-suggestion-cell #_thumbnail .user-thumbnail__image-container {
    background-color: var(--color-fishbase-tuna);
}

.whisper-suggestion-cell .names {
    margin-left: 12px;
    margin-right: 16px;
}

.whisper-suggestion-cell #_username {
    height: 18px;
    -unity-font-definition: var(--font-bold);
    font-size: 15px;
    color: var(--color-fishbase-whitefish);
}

.whisper-suggestion-cell #_displayName {
    height: 16px;
    font-size: 13px;
    color: var(--color-fishbase-anchovy);
}



.multi-color-picker-set-colors {
    background-color: var(--color-fishbase-catfish);
    margin-top: auto;
    padding-bottom: var(--safe-area-bottom);
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
}

.multi-color-picker-set-colors-container {
    flex-direction: row;
    flex-wrap: wrap;
    padding: 0px 8px;
}

.multi-color-picker-set-colors UIMultiColorPickerItem {
    margin: 0;
    margin-left: 8px;
    margin-right: 8px;
    margin-bottom: 12px;
    
}

.multi-color-picker-set-colors-title {
    color: var(--color-fishbase-whitefish);
    -unity-font-style: bold;
    font-size: 16px;
    -unity-text-align: middle-center;
    margin-top: 12px;
    margin-bottom: 16px;
}

.multi-color-picker-set-colors > UIButton {
    margin: 16px;
    margin-top: 4px;
}

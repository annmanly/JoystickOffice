.scroll-picker-cell {
    padding: 4px 0;
}

.scroll-picker-cell UILabel {
    font-size: 26px;
    -unity-text-align: middle-center;
    color: var(--color-fishbase-anchovy);
    -unity-font-definition: var(--font-bold);
}

.scroll-picker-cell:checked UILabel {
    color: var(--color-accents-secondary);
}

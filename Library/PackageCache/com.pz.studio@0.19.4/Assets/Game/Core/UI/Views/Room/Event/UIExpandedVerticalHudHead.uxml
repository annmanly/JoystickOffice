<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIExpandedVerticalHudHead class="expanded-vertical-hud-head">
    <hr:UIImage class="expanded-vertical-hud-head__main-gradient overlay" name="_mainGradient"/>
    <VisualElement class="expanded-vertical-hud-head__image-container">
      <hr:UIImage class="expanded-vertical-hud-head__gradient overlay"/>
      <hr:UIImage name="_image"/>
      <hr:UILabel name="_titleLabel" auto-sizing-font-size="10"/>
    </VisualElement>
    <VisualElement class="expanded-vertical-hud-head__additional-container">
      <hr:UIImage name="_additionalIcon"/>
      <hr:UILabel name="_additionalTextLabel" auto-sizing-font-size="10"/>
    </VisualElement>
  </hr:UIExpandedVerticalHudHead>

</UXML>
.room-info-enable-voice-cell {
    padding: 4px;
}

.room-info-enable-voice-cell-invisible {
    display: none;
    height: 0px;
    padding: 0px;
}

.room-info-enable-voice-cell-button {
    flex-direction: row;
    align-items: center;
    border-radius: 8px;
    background-color: var(--color-fishbase-catfish);
    height: 32px;
}

.room-info-enable-voice-cell-button Image {
    width: 24px;
    height: 24px;
    margin: 4px 6px;
    --unity-image-tint-color: var(--color-fishbase-mackerel);
}

.room-info-enable-voice-cell-button UILabel {
    -unity-font-definition: var(--font-bold);
    color: var(--color-fishbase-bonito);
    margin-right: 6px;
}

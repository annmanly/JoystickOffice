.room-info-header__content {
    width: 100%;
    height: 64px;
    padding-right: 12px;
}

.rtl .room-info-header__content {
    padding-right: 0;
    padding-left: 12px;
}

.room-info-header Image {
    width: 36px;
    height: 36px;
    background-color: var(--color-fishbase-tuna);
    border-radius: 12px;
    padding: 6px;
    --unity-image-tint-color: var(--color-fishbase-bonito);
    margin-top: auto;
    margin-bottom: auto;
    flex-shrink: 0;
}

.room-info-header .icon-share {
    margin-left: 12px;
    margin-right: 8px;
}

.rtl .room-info-header .icon-share {
    margin-left: 8px;
    margin-right: 12px;
}

.room-info-header__details {
    margin-top: auto;
    margin-bottom: auto;
    margin-right: auto;
    margin-left: 16px;
}

.rtl .room-info-header__details {
    margin-right: 16px;
    margin-left: auto;
}

.room-info-header #_roomName {
    padding: 0;
    margin: 0;
    margin-bottom: 4px;
    color: var(--color-fishbase-whitefish);
    font-size: 15px;
    -unity-font-definition: var(--font-bold);
    text-overflow: ellipsis; overflow: hidden; white-space: nowrap;
}

.room-info-header #_roomOwner {
    padding: 0;
    margin: 0;
    color: var(--color-fishbase-mackerel);
    font-size: 13px;
    text-overflow: ellipsis; overflow: hidden; white-space: nowrap;
}

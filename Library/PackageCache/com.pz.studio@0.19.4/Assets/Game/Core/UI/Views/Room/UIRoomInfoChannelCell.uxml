<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIRoomInfoChannelCell class="room-info-channel-cell">
    <VisualElement class="room-info-channel-cell-button rtl-reverse">
      <Image name="_channelIcon" />
      <hr:UILabel name="_channelName" />
      <hr:UIButton name="_reportButton">
        <Image class="icon-report" />
      </hr:UIButton>
      <hr:UIButton name="_moreButton">
        <Image class="icon-more" />
      </hr:UIButton>
    </VisualElement>
  </hr:UIRoomInfoChannelCell>

</UXML>

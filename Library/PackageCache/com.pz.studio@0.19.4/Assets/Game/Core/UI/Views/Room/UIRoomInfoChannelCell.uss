.room-info-channel-cell {
    padding: 4px;
}

.room-info-channel-cell-invisible {
    padding: 0px;
    height: 0px;
    display: none;
}

.room-info-channel-cell-button {
    align-items: center;
    border-radius: 8px;
    height: 32px;
}

.room-info-channel-cell-button Image {
    width: 18px;
    height: 16px;
    margin: 10px 9px;
    --unity-image-tint-color: var(--color-fishbase-anchovy);
}

.room-info-channel-cell-button UILabel {
    -unity-font-definition: var(--font-bold);
    color: var(--color-fishbase-anchovy);
    margin-right: 6px;
    flex-grow: 1;
}

.room-info-channel-cell-button #_reportButton {
    width: 24px;
    height: 24px;
    margin-left: 6px;
    margin-right: 0px;
    padding: 0px;
    background-color: transparent;
}

.room-info-channel-cell-button #_reportButton Image {
    width: 24px;
    height: 24px;
    margin: 0px;
    --unity-image-tint-color: var(--color-accents-secondary);
}

.room-info-channel-cell-button #_moreButton {
    width: 24px;
    height: 24px;
    margin-left: 6px;
    margin-right: 6px;
    padding: 0px;
    background-color: transparent;
}

.room-info-channel-cell-button #_moreButton Image {
    width: 24px;
    height: 24px;
    margin: 0px;
    --unity-image-tint-color: var(--color-accents-secondary);
}

.room-info-channel-cell-joined .room-info-channel-cell-button {
    background-color: var(--color-accents-secondary-25);
    --unity-image-tint-color: var(--color-fishbase-mackerel);
}

.room-info-channel-cell-joined .room-info-channel-cell-button Image {
    --unity-image-tint-color: var(--color-accents-secondary);
}

.room-info-channel-cell-joined .room-info-channel-cell-button UILabel {
    color: var(--color-accents-secondary);
}

.item-command-cell {
    height: 48px;
    align-items: center;
}

.item-command-cell #_itemCard {
    margin: 0 12px;
    height: 36px;
    width: 36px;
    min-width: 36px !important;
}

.item-command-cell #_itemName {
    -unity-font-definition: var(--font-bold);
    font-size: 15px;
    color: var(--color-fishbase-whitefish);
    margin-right: 12px;
}

.rtl .item-command-cell #_itemName {
    margin-right: 0;
    margin-left: 12px;
}

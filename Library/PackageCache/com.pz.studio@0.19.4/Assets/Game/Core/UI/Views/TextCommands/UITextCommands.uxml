<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UITextCommands class="text-commands" picking-mode="Ignore">
    <VisualElement class="top-space" picking-mode="Ignore"/>
    <VisualElement class="text-commands-panel-container" picking-mode="Ignore">
      <VisualElement class="text-commands-panel">
        <hr:UITextCommandsSearch name="_commandSearch" />
        <hr:UITextCommandsAutocomplete name="_commandAutocomplete" />
      </VisualElement>
    </VisualElement>
    <VisualElement name="_bottomSpacer" picking-mode="Ignore"/>
    <hr:UIKeyboardContainer/>
  </hr:UITextCommands>

</UXML>

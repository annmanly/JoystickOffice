<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIRoomInfoHeader class="room-info-header">
    <VisualElement class="room-info-header__content rtl-reverse">
      <VisualElement class="room-info-header__details">
        <hr:UILabel name="_roomName"/>
        <hr:UILabel name="_roomOwner"/>
      </VisualElement>
      <Image class="icon-share" name="_shareButton"/>
      <Image class="icon-report" name="_reportButton" />
    </VisualElement>
  </hr:UIRoomInfoHeader>

</UXML>
/** Colors **/

:root {
    --color-black-10: rgba(0,0,0,.1);
    --color-black-15: rgba(0,0,0,.15);
    --color-black-20: rgba(0,0,0,.2);
    --color-black-25: rgba(0,0,0,.25);
    --color-black-30: rgba(0,0,0,.3);
    --color-black-40: rgba(0,0,0,.4);
    --color-black-50: rgba(0,0,0,.5);
    --color-black-70: rgba(0,0,0,.7);
    --color-black-75: rgba(0,0,0,.75);
    --color-black-80: rgba(0,0,0,.8);
    --color-black-90: rgba(0,0,0,.9);
    --color-transparent: rgba(0,0,0,0);
}

/** Spacing **/

:root {
    --spacing-xxs: 1px;
    --spacing-xs: 2px;
    --spacing-s: 4px;
    --spacing-m: 8px;
    --spacing-l: 12px;
    --spacing-xl: 16px;
    --spacing-xxl: 24px;
}

/** 4 column layout min item width **/
:root {
    --4-column-item-min-width: 79px;
    --4-column-item-max-width: 94px;
}

/** 3 column layout min item width **/
:root {
    --3-column-item-min-width: 101px;
    --3-column-item-max-width: 127px;
}

/** Borders **/

:root {
    --border-width-xs: 1px;
    --border-width-s: 2px;
    --border-width-m: 3px;
    --border-width-l: 4px;
    --border-radius-s: 4px;
    --border-radius-m: 8px;
    --border-radius-l: 16px;
    --border-radius-xl: 20px;
}

/** Fonts **/

:root {
    --font-thin: url("project://database/Packages/com.pz.studio/Assets/Fonts/MuseoSansRounded-300 SDF.asset");
    --font-regular: url("project://database/Packages/com.pz.studio/Assets/Fonts/MuseoSansRounded-500 SDF.asset");
    --font-bold: url("project://database/Packages/com.pz.studio/Assets/Fonts/MuseoSansRounded-700 SDF.asset");
    --font-black: url("project://database/Packages/com.pz.studio/Assets/Fonts/MuseoSansRounded-900 SDF.asset");
    --font-extra-black: url("project://database/Packages/com.pz.studio/Assets/Fonts/MuseoSansRounded-1000 SDF.asset");
    --font-thin-italic: url("project://database/Packages/com.pz.studio/Assets/Fonts/MuseoSansRounded-300_Italic_22 SDF.asset");
    --font-regular-italic: url("project://database/Packages/com.pz.studio/Assets/Fonts/MuseoSansRounded-500_Italic_22 SDF.asset");
    --font-bold-italic: url("project://database/Packages/com.pz.studio/Assets/Fonts/MuseoSansRounded-700_Italic_22 SDF.asset");
    --font-black-italic: url("project://database/Packages/com.pz.studio/Assets/Fonts/MuseoSansRounded-900_Italic_22 SDF.asset");
    --font-koulen: url("project://database/Packages/com.pz.studio/Assets/Fonts/Koulen-Regular SDF.asset");
    --font-size-xs: 11px;
    --font-size-s: 13px;
    --font-size-default: 15px;
    --font-size-m: 15px;
    --font-size-l: 17px;
    --font-size-xl: 22px;
    --font-size-big-emojis: 34px;
}

/** Landscape **/

:root {
    --lanscape-dialog-max-width: 600px;
    --landscape-chat-min-width: 500px;
}



<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:hrui="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIColorCellList name="color-cell-list" class="color-cell-list flex-grow">
    <hrui:UICollection class="flex-grow">
        <hrui:UICollectionSection>
          <hrui:UICollectionCells name="_colorGrid" layout="grid" layout-spacing="2"/>
        </hrui:UICollectionSection>
    </hrui:UICollection>
  </hr:UIColorCellList>

</UXML>

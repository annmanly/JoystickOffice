.saved-outfit-cell-list {
    
}

.saved-outfit-cell-list .icon-outfit-savedoutfits-artwork {
    background-color: var(--color-fishbase-catfish);
    --unity-image-tint-color: var(--color-fishbase-eel);
    width: 64px; 
    height: 64px; 
    padding: 16px;
    margin-left: auto;
    margin-right: auto;
    border-radius: 20px; 
    margin-top: 24px;
    margin-bottom: 16px;
}

.saved-outfit-cell-list__save-label {
    color: var(--color-fishbase-tuna);
    -unity-text-align: upper-center;
    font-size: 15px;
    overflow: hidden;
    white-space: normal;
    margin-top: 0px;
    margin-left: 64px;
    margin-right: 64px;
    margin-bottom: 24px;
    padding: 0px;
    bottom: 8px;
}

.saved-outfit-cell-list__save-button {
    margin-top: 0px;
    margin-bottom: auto;
    margin-left: auto;
    margin-right: auto;
    padding: 13px 26px;
}

#_saveOutfitDisplay {
    display: none;
    flex-grow: 1
}

.saved-outfit-cell-list #_headerSection {
    display: none;
    height: 50px;
    padding-top: 8px;
    align-items: center;
    flex-shrink: 0;
}

.saved-outfit-cell-list__header-label {
    font-size: 17px;
    margin-left: 16px;
    -unity-font-definition: var(--font-bold);
    color: var(--color-fishbase-whitefish);
}

.saved-outfit-cell-list #_outfitSlotsLabel {
    height: 18px;
    font-size: 17px;
    color: var(--color-fishbase-salmon);
    -unity-font-definition: var(--font-regular);
    -unity-text-align: middle-center;
    margin-right: 16px;
    margin-left: auto;
}

.saved-outfit-cell-list #_addSlots {
    display: none;
    width: 24px;
    flex-shrink: 0;
    height: 24px;
    padding: 6px;
    margin: auto 16px auto 0px;
    border-radius: 8px;
    background-color: var(--color-accents-secondary);
    --unity-image-tint-color: var(--color-fishbase-eel);
    --unity-image: var(--image-icon-wallet);
}

.saved-outfit-cell-list .scroll-bar {
    margin-right: 4px;
    margin-top: 16px;
    margin-bottom: 56px;
}


.saved-outfit-cell-list #_outfitsSection {
    padding-bottom: var(--safe-area-bottom);
}
.saved-outfit-cell {
    width: 80px;
    height: 140px;
    border-width: 3px;
    border-color: var(--color-fishbase-catfish);
    background-color: var(--color-fishbase-catfish);
}

.saved-outfit-cell__image {
    height: 100%;
    padding: 8px;
}

.saved-outfit-cell:checked {
    border-color: var(--color-accents-primary);   
}

.saved-outfit-cell #_deleteButton {
    width: 16px;
    height: 16px;
    padding: 0px;
    margin: 2px;
    border-radius: 4px;
    background-color: var(--color-fishbase-salmon);
}

.saved-outfit-cell #_deleteButton UIImage {
    width: 16px;
    height: 16px;
    --unity-image-tint-color: var(--color-fishbase-eel);
}

.saved-outfit-cell #_alertIcon {
    width: 21px;
    height: 21px;
    border-radius: 4px;
    --unity-image-tint-color: var(--color-basic-destructive);
}

<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIItemInfoQuantity class="item-info-quantity">
    <VisualElement class="rtl-reverse">
      <hr:UITextField name="_quantityInput" />
      <hr:UIButton class="button-primary-25" name="_quantityPositiveButton" focusable="false">
          <hr:UILabel name="_quantityPositiveButtonLabel" />
      </hr:UIButton>
    </VisualElement>
    <hr:UIButton name="_quantityNegativeButton" class="button-destructive" focusable="false">
        <hr:UILabel name="_quantityNegativeButtonLabel" />
    </hr:UIButton>
  </hr:UIItemInfoQuantity>

</UXML>

<?xml version="1.0" encoding="utf-8"?>
<UXML
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="UnityEngine.UIElements"
        xmlns:hr="Highrise.UI"
        xmlns:editor="UnityEditor.UIElements"
        xsi:noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd"
>

    <hr:UIClosetItemCell class="closet-item-cell rarity-bg">
        <hr:UIMythicalBackground class="overlay"/>

        <hr:UIImage name="_clothingImage"/>
        <hr:UIImage name="_accountBoundBadge" class="overlay-bottom-left icon-non-tradable"/>

        <hr:UILabel name="_nfiBadge" class="overlay-bottom-left"/>
        <hr:UIImage name="_nftBadge" class="overlay-bottom-left icon-non-tradable"/>
        
        <hr:UILabel name="_newBadge" class="overlay-bottom-right"/>
        <hr:UIImage name="_effectsIcon" class="icon-outfit-auras" />
        
        <VisualElement name="_selectedOverlay" class="overlay"/>

        <hr:UICurrency name="_price"/>
        <hr:UIImage name="_gachaBadge" class="icon-gacha"/>

        <hr:UIImage name="_selectedBadge" class="overlay-bottom-right icon-checkmark"/>

    </hr:UIClosetItemCell>

</UXML>
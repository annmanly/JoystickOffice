.outfit-not-owned-alert {
    margin-left: 16px;
    margin-right: 16px;
    padding-bottom: var(--safe-area-bottom);
    margin-top: auto;
}

.landscape .outfit-not-owned-alert {
    max-width: 340px;
    width: 340px;
    margin: auto;
}

.outfit-not-owned-alert-content {
    background-color: var(--color-fishbase-catfish);
    border-radius: 20px;
    padding: 16px;
    margin-bottom: 16px;
}

.outfit-not-owned-alert #_itemGrid {
    height: 80px;
    width: 248px;
    margin-top: 4px;
    margin-left: auto;
    margin-right: auto;
}

.outfit-not-owned-alert #_itemGrid .item-card {
    height: 80px;
    width: 80px;
}

.outfit-not-owned-alert-two-lines #_itemGrid {
    height: 160px;
}

.outfit-not-owned-alert-many-lines #_itemGrid {
    height: 191px;
}

.outfit-not-owned-alert #_itemGrid .scroll-bar {
    display: none;
}

.outfit-not-owned-alert .outfit-not-owned-alert-scrim {
    position: absolute;
    height: 37px;
    width: 285px;
    top: 174px;
    left: auto;
    right: auto;
    display: none;
    background-image: var(--image-vgradient-180);
    -unity-background-image-tint-color: rgba(40, 39, 45, 1);
}

.outfit-not-owned-alert-many-lines .outfit-not-owned-alert-scrim {
    display: flex;
}

.outfit-not-owned-alert #_titleLabel {
    margin-top: 28px;
    color: var(--color-fishbase-whitefish);
    font-size: 20px;
    -unity-font-definition: var(--font-bold);
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: normal;
    -unity-text-align: middle-center;
}

.outfit-not-owned-alert-description {
    margin-top: 4px;
    margin-bottom: 28px;
    color: var(--color-fishbase-mackerel);
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: normal;
    -unity-text-align: middle-center;
}

<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIItemInfoShowcase class="item-info-showcase">
    <hr:UIButton name="_showcaseButton" class="button-primary">
      <hr:UILabel key="add_to_showcase" />
    </hr:UIButton>

    <hr:UIButton name="_removeFromShowcasesButton" class="button-destructive">
      <hr:UILabel key="remove_from_showcase" />
    </hr:UIButton>

    <hr:UIButton name="_messageUserButton" class="button-primary">
      <hr:UILabel key="message_user" />
    </hr:UIButton>
  </hr:UIItemInfoShowcase>

</UXML>

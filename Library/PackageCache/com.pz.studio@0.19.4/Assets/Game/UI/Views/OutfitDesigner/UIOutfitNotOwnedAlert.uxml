<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIOutfitNotOwnedAlert class="outfit-not-owned-alert">
    <VisualElement class="outfit-not-owned-alert-content">
      <hr:UICollection name="_itemGrid">
        <hr:UICollectionSection>
          <hr:UICollectionCells name="_itemGridCells" layout="grid" fixed-height="true" layout-spacing="4" />
        </hr:UICollectionSection>
      </hr:UICollection>
      <hr:UIImage class="outfit-not-owned-alert-scrim" />
      <hr:UILabel name="_titleLabel"/>
      <hr:UILabel key="you_cannot_save_your_outfit_items_unavailable" class="outfit-not-owned-alert-description" />
      <hr:UIButton name="_okayButton" class="button-primary">
        <hr:UILabel key="okay" />
      </hr:UIButton>
    </VisualElement>
  </hr:UIOutfitNotOwnedAlert>

</UXML>

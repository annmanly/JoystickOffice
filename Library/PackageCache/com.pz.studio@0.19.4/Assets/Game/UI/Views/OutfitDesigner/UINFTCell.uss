.nft-cell
{
    width: 109px;
    min-width: 109px;
    height: 109px;
    overflow: hidden;
    border-color: var(--color-fishbase-catfish); 
    
}

.nft-cell #_selectedBadge {
    display: none;
    width: 22px;
    height: 22px;
    border-radius: 8px;
    bottom: 5px;
    right: 5px;
    padding: 3px;
    background-color: var(--color-accents-primary);
    --unity-image-tint-color: var(--color-fishbase-eel);
}

.nft-cell:checked #_selectedBadge {
    display: flex;
}
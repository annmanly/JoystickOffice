.closet {
    flex-grow: 1;        
}

.portrait .closet {
    padding-top: var(--safe-area-top);
}

.closet #_backButton {
    display: none;
}

.closet__preview {
    flex-grow: 1;
}

.closet__preview #_avatarImage {
    height: 100%;
}

.closet__status-bar-overlay {
    width: 100%;
    height: var(--safe-area-top);
}

.closet__title {
    display: none;
    font-size: 17px;
    margin-top: 16px;
    padding-top: var(--safe-area-top);
    -unity-text-align: middle-center;
    color: var(--color-fishbase-whitefish);
    -unity-font-definition: var(--font-bold);
}

.outfit-designer__preview-colors .color-cell {
    width: 32px;
    height: 32px;
}

.outfit-designer__preview-colors {
    height: 44px;
    margin-top: auto;
}

.outfit-designer__preview-colors .collection-section {
    padding-left: 12px;
    padding-right: 12px;
    padding-bottom: 8px;
}

.closet__customization-item__bg {
    width: 64px;
    height: 64px;
}

.closet__filters {
    background-color: var(--color-fishbase-blackfish);
    width: 248px;
    flex-grow: 0;
    flex-shrink: 0;
}

.closet__customization {
    background-color: var(--color-fishbase-eel);
    padding-right: 0px;
    padding-left: 0px;
    margin: 0px;
    margin-top: auto;
    flex-shrink: 0;
    height: 336px;
}

.portrait .closet__customization {
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
}

.closet__margins {
    margin: var(--spacing-m);
}

#_rightButtons {
    flex-direction: row;
}

#_rightButtons #_undoButton,
#_rightButtons #_saveButton {
    font-size: 15px;
}

#_rightButtons UIButton {
    margin-left: 8px;
    height: 36px;
    width: auto;
    padding: 0px 8px;
}

.portrait #_rightButtons UIButton {
    color: var(--color-fishbase-whitefish);
    background-color: var(--color-fishbase-blackfish-40);
    border-radius: 12px;
}

.portrait #_rightButtons UIButton:disabled {
    color: var(--color-fishbase-mackerel);
}

#_rightButtons UIButton > Image {
    width: 24px;
    height: 24px;
    margin: auto;
}

.closet__preview #_undoButton {
    margin-right: 8px;
}

.closet__preview #_undoButton, 
.closet__preview #_saveButton {
    padding-left: 12px;
    padding-right: 12px;
    font-size: 15px;
}

.closet #_extraButtons {
    position: absolute;
    top: var(--safe-area-top);
    right: 12px;
    padding-top: 44px;
}

.closet #_extraButtons UIButton {
    margin-top: 8px;
    padding: 6px;
    width: 36px;
    height: 36px;
}

.closet #_extraButtons UIButton UIImage {
    margin: 0px;
    height: 24px;
    width: 24px;
    --unity-image-tint-color: var(--color-fishbase-bonito);
}

.closet__filter-text {
    font-size: var(--font-size-m);
    color: var(--color-fishbase-bonito);
    -unity-text-align: middle-left;
}

.closet__foldout {
    overflow: hidden;
    color: var(--color-fishbase-whitefish);
}

.closet__foldout .unity-foldout__content {
    margin: 0px;
}


.closet__tab-button  {
    flex-grow: 1;
}

.closet__tab-button Image {
    --unity-image-tint-color: var(--color-fishbase-whitefish);
    flex-grow: 1;
}

.closet__tab-button:checked Image {
    --unity-image-tint-color: var(--color-accents-secondary);
}

.outfit-designer__tab {
    --unity-image-tint-color: var(--color-fishbase-salmon);
    flex-shrink: 0;
    width: 44px;
    height: 48px;
    align-self: center;
    padding: 8px 6px;
}

.outfit-designer__tab:checked{
    --unity-image-tint-color: var(--color-accents-secondary);
}

.closet__filterbar {
    margin-top: 12px;
    margin-left: 16px;
    margin-right: 16px;
    margin-bottom: 12px;
}

.closet__filterbar UIButton
{
    padding-left: 8px;
    padding-right: 8px;
    background-color: var(--color-fishbase-catfish);
    border-radius: 8px;
    font-size: 15px;
}

.closet__filterbar Label {
    font-size:15px;
    margin-right: 0px;
    padding-left: 0px;
}


.closet__filterbar UIPopupPicker {
    margin-left: 8px;
    border-radius: 8px;
}

.closet #_clearButton Label {
    padding:0;
    margin:0;
}

.closet #_clearButton {
    padding-left: 8px;
    padding-right: 8px;
    margin-top: 0px;
    margin-bottom: 0px;
    height: 32px;
    background-color: var(--color-fishbase-eel);
    color: var(--color-accents-secondary);
}

.closet__filterbar UIButton Image {
    margin:0;
    padding:0;
}

.closet .search-control-expanded {
    background-color: var(--color-fishbase-eel);
}

.closet .search-control-expanded-back-button {
    background-color: var(--color-fishbase-eel);
}

.closet #_sortDropdown {
}   

.closet #_sortPicker {
    flex-shrink: 0;
}   

.closet__customization-favouritesbutton Image {
    --unity-image-tint-color: var(--color-fishbase-mackerel);
    width:16px;
    height:16px;
}

.closet__customization-favouritesbutton {
    margin-top: 0;
    margin-bottom: 0;
    width: 32px;
    height: 32px;
    margin-right: 8px;
    border-radius: 8px;
}

.rtl .closet__customization-favouritesbutton {
    margin-left: 8px;
    margin-right: 0;
}

.closet__customization-favouritesbutton:checked Image {
    --unity-image-tint-color: var(--color-accents-secondary);
}

.closet__customization-top {
    flex-grow: 1;
    flex-shrink: 0;
}

.closet__customization-filterbottom UIMultiColorPicker {
    margin-left: 8px;
}

.closet__pages {
    flex-grow:1;
    flex-shrink: 0;
    margin-top: 0px;
    margin-left: 0px;
    margin-right: 0px;
}

.closet #_sortPicker {
    margin-left: 0;
}

.rtl .closet #_sortPicker {
    margin-left: 8px;
}

.closet #_colorPicker {
   
}

.closet #_genderPicker {

}

.closet #_typePicker {
    margin-right:auto;
}

.closet #_tabsContainer {
    background-color: var(--color-fishbase-eel);
    border-top-width: 0.5px;
    border-top-color: var(--color-fishbase-catfish);
}

.closet__tabs-scrollview {
    flex-grow: 0;
}

.closet__tabs-scrollview UIScrollBar {
    display: none;
}

.closet__tabs-scrollview .scroll-view__content-viewport {
}

.closet__tabs-scrollview .scroll-view__content-container {
    padding-left: 4px;
    padding-right: 4px;
    height: 48px;
}

.closet__tabs-scrollview .scroll-bar__thumb {
    color: var(--color-fishbase-salmon);
}

.closet UIKeyboardContainer {
    min-height: var(--safe-area-bottom);
    background-color: var(--color-fishbase-eel);
    flex-shrink: 0;
    display: flex;
}

.closet__customization-filtertop .text-field__input{
    margin-left: 8px;
}

.rtl .closet__customization-filtertop .text-field__input{
    margin-right: 8px;
}

/* Landscape */
.landscape .closet__filterbar {
    margin-bottom: 4px;
}

.landscape .closet__filterbar .scroll-bar {
    height: 4px;
}

.landscape .closet__filterbar .scroll-view__content-container {
    padding-bottom: 8px;
}

/* Pet Inventory */

.closet UIClosetPetInventory {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: var(--color-fishbase-eel);
}

.closet__bottom-overlay {
    margin-bottom: 336px;
    bottom: var(--safe-area-bottom);    
}

.landscape .closet__bottom-overlay {
    margin-bottom: 0;
    bottom: 0;
    padding-left: 0px;
    padding-right: 10px;
}

.landscape.rtl .closet__bottom-overlay {
    padding-left: 10px;
    padding-right: 0;
}

.closet__ootd-button {
    display: none;
    width: 52px;
    height: 58px;
    margin-left: 12px;
    margin-bottom: 12px;
}

.rtl .closet__ootd-button {
    left: auto;
    margin-left: 0;
    margin-right: 12px;
}

.closet__ootd-button .button__content {
    flex-direction: column;
    align-items: center;
    margin: 0;
    flex-grow: 1;
}

.closet__ootd-button UIImage {
    width: 24px;
    height: 24px;
    margin: auto;
}

.closet__ootd-button Label {
    font-size: 11px;
    color: var(--color-fishbase-whitefish);
    -unity-font-definition: var(--font-bold);
}

.landscape .closet {
    flex-direction: row-reverse;
}

.rtl.landscape .closet {
    flex-direction: row;
}

.landscape .closet__preview {
    position: relative;
    background-color: var(--color-fishbase-eel);
    width: 430px;
    flex-grow: 0;
    margin-bottom: 0;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.landscape .modal .closet__preview {
    width: 50%;
    align-items: initial;
}

.landscape .closet.closet__shared-avatar-view .closet__preview {
    background-color: transparent;
}

.landscape .closet #_avatarContainer {
    position: relative;
    width: 375px;
    height: 667px;
    max-height: 100%;
    flex-grow: 0;
}

.landscape .modal .closet #_avatarContainer {
    height: initial;
    width: initial;
    flex-grow: 1;
}

.landscape .closet UIAvatarView {
    border-radius: 20px;
    overflow: hidden
}

.landscape .closet__customization-container {
    position: relative;
    flex-grow: 1;
    width: initial;
}

.landscape .modal .closet__customization-container {
    width: 50%;
}

.landscape .closet__customization {
    height: initial;
    flex-grow: 1;
}

.landscape .outfit-designer__preview-colors {
    height: 64px;
    bottom: 0;
    margin-bottom: 0;
}

.landscape .outfit-designer__preview-colors .color-cell {
    width: 52px;
    height: 52px;
}

.landscape .closet .color-cell--premium.PremiumColorPosition-Start,
.landscape .closet .color-cell--premium.PremiumColorPosition-StartAndEnd {
    width: 88px;
}

.landscape .panel-open .closet__preview {
    width: 0;
}

.landscape .closet #_extraButtons {
    padding-top: 12px;
}

.landscape .closet UIKeyboardContainer {
    display: none;
}
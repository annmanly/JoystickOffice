.rewards-summary {
    background-color: var(--color-fishbase-blackfish);
    flex-grow: 1;
}

.rewards-summary UICollection {
    margin-top: var(--safe-area-top);
}

.rewards-summary UICollectionSection {
    padding: 0 15px 170px 16px;
}

.rewards-summary #_title {
    opacity: 0;
    color: white;
    padding-top: 12px;
    margin: 80px auto 48px;
    font-size: 17px;
    -unity-font-definition: var(--font-bold);
}

.rewards-summary #_buttonContainer {
    padding-bottom: 16px;
    background-color: var(--color-fishbase-blackfish);
}

.rewards-summary  #_topSeparator {
    background-color: var(--color-fishbase-catfish);
    margin-bottom: 16px;
}

.rewards-summary #_gotItButton {
    margin-left: 16px;
    margin-right: 16px;
    margin-top: 12px;
}

.rewards-summary #_swapItemsButton {
    margin-left: 16px;
    margin-right: 16px;
    background-color: var(--color-tertiary-blueberry-25);
}

.rewards-summary #_swapItemsButton > UILabel {
    margin-left: 4px;
    color: var(--color-tertiary-blueberry);
}

.rewards-summary #_swapItemsButton > UIImage {
    width: 20px;
    height: 20px;
    --unity-image-tint-color: var(--color-tertiary-blueberry);
}

.rewards-summary #_giftExchangeLabel {
    margin-left: 16px;
    margin-right: 16px;
    -unity-text-align: middle-center;
    white-space: normal;
    color: var(--color-fishbase-bonito);
    font-size: 13px;
}

.rewards-summary #_exchangeGiftsButton {
    margin-left: 16px;
    margin-right: 16px;
    margin-top: 12px;
}
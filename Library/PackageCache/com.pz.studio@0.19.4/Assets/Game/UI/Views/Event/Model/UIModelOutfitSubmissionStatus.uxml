<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIModelOutfitSubmissionStatus class="model-outfit-submission-status">
    <hr:UIUserThumbnail name="_outfit"/>
    
    <VisualElement class="model-outfit-submission-status__text-container">
      <VisualElement class="model-outfit-submission-status__title-container">
        <hr:UILabel name="_title"/>
        <hr:UIImage name="_alertBadge" class="icon-small-alert"/>
      </VisualElement>
      <hr:UILabel name="_description"/>
    </VisualElement>

    <hr:UIButton class="button-icon" name="_submitOutfitButton">
      <hr:UIImage class="icon-back"/>
    </hr:UIButton>
    
  </hr:UIModelOutfitSubmissionStatus>

</UXML>
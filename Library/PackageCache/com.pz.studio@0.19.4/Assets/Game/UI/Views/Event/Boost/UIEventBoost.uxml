<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIEventBoost class="event-boost">
    <hr:UIScrollView>
      <hr:UIHint key="boost_hint" icon-class="icon-boost-event-hint"/>
      <VisualElement name="_tabBar" class="event-boost__tab-bar">
        <hr:UIButton name="_judgeButton" class="event-boost__tab-button">
          <hr:UILabel key="judge" />
        </hr:UIButton>
        <hr:UIButton name="_modelButton" class="event-boost__tab-button">
          <hr:UILabel key="model" />
        </hr:UIButton>
      </VisualElement>
      <VisualElement name="_pagesContainer"/>
    </hr:UIScrollView>
  </hr:UIEventBoost>

</UXML>
<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIEventProgressRewards class="event-progress-rewards">
    <hr:UICollection name="_rewardsCollection" layout-spacing="8" haptics="false">
      <hr:UICollectionSection name="_hintSection">
        <hr:UIHint name="_hint" key="earn_rewards_instantly" icon-class="icon-hint-present"/>
      </hr:UICollectionSection>
      <hr:UICollectionSection name="_rewardsSection">
        <hr:UICollectionCells layout="stack" name="_rewardsCells"/>
      </hr:UICollectionSection>
    </hr:UICollection>
    
    <VisualElement name="_nextRewardContainer" class="overlay-bottom-safe">
      
    </VisualElement>
  </hr:UIEventProgressRewards>

</UXML>
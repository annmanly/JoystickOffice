<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIEventSuperBoostHudHead class="event-super-boost-hud-head">
    <VisualElement name="_indicator" class="event-super-boost-hud-head__indicator">
      <hr:UIImage class="event-super-boost-hud-head__indicator-icon icon-super-boost-indicator" />
      <hr:UILabel class="event-super-boost-hud-head__indicator-label overlay-top" key="start_exclamation" />
    </VisualElement>
    <VisualElement class="event-super-boost-hud-head__progress-container">
      <hr:UIBlur class="overlay"/>
      <hr:RadialProgress name="_progress" class="event-super-boost-hud-head__progress">
        <VisualElement name="_button" class="event-super-boost-hud-head__button">
          <hr:UIImage class="event-super-boost-hud-head__button-gradient overlay" />
          <hr:UIImage name="_buttonIcon" class="event-super-boost-hud-head__button-icon" />
        </VisualElement>
      </hr:RadialProgress>
    </VisualElement>
  </hr:UIEventSuperBoostHudHead>

</UXML>
<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIEventLeaderboardRankItemCell class="event-leaderboard-rank-item-cell">
    <VisualElement name="_card" class="event-leaderboard-rank-item-cell__card">
      <hr:UILabel name="_rank" class="event-leaderboard-rank-item-cell__rank" />
      <hr:UIUserThumbnail name="_thumbnail" class="event-leaderboard-rank-item-cell__thumbnail" />
      <hr:UILabel name="_name" class="event-leaderboard-rank-item-cell__name" />
      <VisualElement class="event-leaderboard-rank-item-cell__tickets-container">
        <hr:UIImage name="_ticketsIcon" class="event-leaderboard-rank-item-cell__tickets-icon event-tickets-icon"/>
        <hr:UILabel name="_ticketsLabel" class="event-leaderboard-rank-item-cell__tickets-label"/>
      </VisualElement>
    </VisualElement>
    <VisualElement name="_divider">
        <VisualElement class="separator"/>
    </VisualElement>
  </hr:UIEventLeaderboardRankItemCell>

</UXML>
<?xml version="1.0" encoding="utf-8"?>
<UXML
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="UnityEngine.UIElements"
        xmlns:hr="Highrise.UI"
        xmlns:editor="UnityEditor.UIElements"
        xsi:noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd"
>

    <hr:UIEventProgressRewardCell class="event-progress-reward-cell">
        <VisualElement name="_rewardContainer">
            <hr:UIItemCard name="_itemCard"/>
            <hr:UIImage name="_itemImage"/>
            <hr:UILabel name="_amountLabel" class="overlay-top-right"/>
            <VisualElement name="_ticketsContainer">
                <hr:UIImage name="_icon"/>
                <hr:UILabel name="_ticketCount"/>
            </VisualElement>
        </VisualElement>
        
        <VisualElement name="_progressContainer">
            <VisualElement name="_progressTop"/>
            <VisualElement name="_progressBottom"/>
            <hr:UIImage name="_progressMiddleSmall" class="overlay icon-event-progress-reward-notch-small"/>
            <hr:UIImage name="_progressMiddleLarge" class="overlay icon-event-progress-reward-notch-large"/>
            <hr:UIImage name="_progressMiddleFinal" class="overlay icon-event-progress-reward-notch-final"/>
        </VisualElement>
    </hr:UIEventProgressRewardCell>

</UXML>
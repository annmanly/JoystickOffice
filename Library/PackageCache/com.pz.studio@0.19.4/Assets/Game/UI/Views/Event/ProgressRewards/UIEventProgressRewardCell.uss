.event-progress-reward-cell {
    justify-content: center;
    flex-direction: row;
    overflow: visible;
}

.event-progress-reward-cell #_rewardContainer {
    margin-top: 4px;
    margin-bottom: 4px;
    width: 136px;
    background-color: var(--color-fishbase-eel);
    align-items: center;
    border-radius: 12px;
}

.event-progress-reward-cell #_itemCard {
    margin-top: 8px;
    width: 120px;
    height: 164px;
}

.event-progress-reward-cell #_itemCard #_name {
    font-size: 13px;
    -unity-text-align: middle-left;
}

.event-progress-reward-cell #_itemImage {
    margin-top: 8px;
    width: 32px;
    height: 32px;
}

.event-progress-reward-cell #_amountLabel {
    border-radius: 4px;
    background-color: var(--color-fishbase-catfish);
    color: var(--color-fishbase-anchovy);
    font-size: 11px;
    padding-left: 2px;
    padding-right: 2px;
    margin-top: -3px;
    margin-right: -3px;
    -unity-font-definition: var(--font-bold);
}

.event-progress-reward-cell #_ticketsContainer {
    flex-direction: row;
    align-items: center;
    margin-top: 4px;
    margin-bottom: 4px;
    padding-top: 3px;
    padding-bottom: 3px;
    padding-left: 8px;
    padding-right: 8px;

}

.event-progress-reward-cell #_icon {
    width: 16px;
    height: 16px;
}

.event-progress-reward-cell #_ticketCount {
    color: var(--color-fishbase-bonito);
    font-size: 15px;
    margin-left: 4px;
    -unity-font-definition: var(--font-bold);
}

.event-progress-reward-cell #_progressContainer {
    margin-left: 20px;
    width: 28px;
    align-items: center;
}

.event-progress-reward-cell #_progressTop {
    width: 6px;
    margin-top: -2px;
    flex-grow: 1;
}

.event-progress-reward-cell #_progressBottom {
    width: 6px;
    flex-grow: 1;
    border-bottom-left-radius: 2px;
    border-bottom-right-radius: 2px;
}

.event-progress-reward-cell #_progressMiddleSmall {
    display: none;
    width: 24px;
    height: 16px;
    left: 50%;
    margin-left: -12px;
    top: 50%;
    margin-top: -8px;
}

.event-progress-reward-cell #_progressMiddleLarge {
    display: none;
    width: 24px;
    height: 28px;
    left: 50%;
    top: 50%;
    margin-left: -12px;
    margin-top: -14px;
}


.event-progress-reward-cell #_progressMiddleFinal {
    display: none;
    width: 24px;
    height: 28px;
    left: 50%;
    top: 50%;
    margin-left: -12px;
    margin-top: -14px;
}
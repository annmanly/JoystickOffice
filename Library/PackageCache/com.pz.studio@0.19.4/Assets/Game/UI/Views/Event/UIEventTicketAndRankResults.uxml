<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIEventTicketAndRankResults class="event-ticket-and-rank-results">
    <VisualElement name="_actionRewardContainer">
      <hr:UIImage name="_actionRewardIcon"/>
      <hr:UILabel name="_actionRewardLabel"/>
    </VisualElement>
    
    <VisualElement name="_resultTicketsContainer" class="flex-row">
      <hr:UIImage name="_ticketsIcon" class="event-tickets-icon"/>
      <hr:UILabel name="_ticketsTotal"/>
      <hr:UILabel name="_ticketsWon"/>
    </VisualElement>

    <VisualElement name="_resultRankContainer" class="flex-row">
      <hr:UIImage name="_rankIcon"/>
      <hr:UILabel name="_currentRank"/>
      <hr:UIImage name="_rankDiffIcon"/>
      <hr:UILabel name="_rankDiff"/>
    </VisualElement>
  </hr:UIEventTicketAndRankResults>

</UXML>
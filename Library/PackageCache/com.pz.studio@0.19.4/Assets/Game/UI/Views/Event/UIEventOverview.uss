.event-overview {
    flex-grow: 1;
}

.event-overview__rank-title {
    margin-top: 20px;
    margin-bottom: 16px;
    color: var(--color-fishbase-whitefish);
    font-size: 22px;
    margin-right: 16px;
    -unity-font-style: bold;
}

.event-overview__card-background {
    margin-top: 12px;
    background-color: var(--color-fishbase-catfish);
    border-radius: 12px;
    overflow: hidden;
}

.event-overview__card-background .separator {
    margin-left: 12px;
    background-color: var(--color-fishbase-tuna);
}

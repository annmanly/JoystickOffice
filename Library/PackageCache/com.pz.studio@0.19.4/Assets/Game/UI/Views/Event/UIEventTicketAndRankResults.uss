.event-ticket-and-rank-results {
}

.event-ticket-and-rank-results:disabled {
    opacity: 1;
}

.event-ticket-and-rank-results #_actionRewardContainer {
    justify-content: center;
    align-items: center;
    flex-direction: row;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 16px;
    border-radius: 12px;
    padding: 6px;
    background-color: var(--color-fishbase-tuna);
}

.event-ticket-and-rank-results #_actionRewardIcon {
    width: 24px;
    height: 24px;
    margin-left: 6px;
    margin-right: 8px;
}

.event-ticket-and-rank-results #_actionRewardLabel {
    font-size: 15px;
    margin-right: 6px;
    color: var(--color-fishbase-bonito);
    -unity-font-definition: var(--font-bold-italic);
}

.event-ticket-and-rank-results #_resultTicketsContainer {
    justify-content: center;
}

.event-ticket-and-rank-results #_ticketsIcon {
    width: 16px;
    height: 16px;
}

.event-ticket-and-rank-results #_ticketsTotal {
    font-size: 15px;
    margin-left: 4px;
    color: var(--color-fishbase-mackerel);
}

.event-ticket-and-rank-results #_ticketsWon {
    margin-left: 8px;
    font-size: 15px;
    -unity-font-definition: var(--font-bold-italic);
    color: var(--color-fishbase-whitefish);
}

.event-ticket-and-rank-results #_resultRankContainer {
    justify-content: center;
    margin-top: 4px;
}

.event-ticket-and-rank-results #_rankIcon {
    display: none;
    width: 16px;
    height: 16px;
}

.event-ticket-and-rank-results #_currentRank {
    color: var(--color-fishbase-anchovy);
    font-size: 15px;
}

.event-ticket-and-rank-results #_rankDiffIcon {
    width: 16px;
    height: 16px;
    margin-left: 8px;
}

.event-ticket-and-rank-results #_rankDiff {
    margin-left: 4px;
    font-size: 15px;
    -unity-font-definition: var(--font-bold-italic);
    color: var(--color-accents-primary);
}
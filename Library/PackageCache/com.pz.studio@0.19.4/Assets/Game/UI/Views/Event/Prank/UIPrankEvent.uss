.prank-event {
    flex-grow: 1;
    width: 100%;
    align-self: center;
}

.landscape .prank-event {
    max-width: var(--lanscape-dialog-max-width);
}

.prank-event #_prankTouchBlocker {
    flex-grow: 1;
}

.prank-event-content {
    margin-top: auto;
    border-radius: 16px;
    background-color: var(--color-fishbase-eel);
    padding-bottom: 8px;
}

.prank-event #_stageLabel {
    -unity-font-style: bold;
    color: var(--color-fishbase-bonito);
    font-size: 17px;
    margin-top: 16px;
    -unity-text-align: middle-center;
}

.prank-event #_chooseItemLabel {
    margin-top: 16px;
    margin-left: 16px;
    margin-right: 16px;
    -unity-text-align: middle-center;
    -unity-font-definition: var(--font-regular-italic);
    color: var(--color-fishbase-salmon);
    font-size: 15px;
}

.prank-event #_itemsContainer {
    height: 136px;
    flex-direction: row;
    justify-content: center;
    margin-top: 8px;
    margin-left: 12px;
    margin-right: 12px;
}

.prank-event #_itemsContainer > * {
    margin-left: 4px;
    margin-right: 4px;
}

.prank-event #_resultsContainer {
    display: none;
    border-top-left-radius: 16px;
    border-top-right-radius: 16px;
    background-color: var(--color-fishbase-eel);
}

.prank-event #_resultTop {
    border-top-left-radius: 16px;
    border-top-right-radius: 16px;
    background-color: var(--color-accents-primary-15);
    align-items: center;
}

.prank-event #_resultImage {
    width: 60px;
    height: 60px;
    padding: 14px;
    border-radius: 16px;
    background-color: var(--color-fishbase-eel);
    margin-top: 12px;
}

.prank-event #_resultTitle {
    color: var(--color-accents-primary);
    -unity-font-definition: var(--font-bold-italic);
    font-size: 22px;
    margin-top: 8px;
    margin-bottom: 12px;
}

.prank-event #_stageUpdate {
    -unity-font-definition: var(--font-black);
    font-size: 15px;
    opacity: 1;
    margin-bottom: 0px;
    margin-left: 8px;
    margin-right: 8px;
    margin-top: 16px;
    -unity-text-align: middle-center;
}

.prank-event #_tapToContinueLabel {
    -unity-text-align: middle-center;
    white-space: normal;
    margin-top: 16px;
    margin-left: 16px;
    margin-right: 16px;
    margin-bottom: 16px;
    padding-top: 11px;
    padding-bottom: 11px;
    color: var(--color-fishbase-salmon);
    -unity-font-definition: var(--font-regular-italic);
}

.prank-event #_results {
    margin-top: 16px;
}

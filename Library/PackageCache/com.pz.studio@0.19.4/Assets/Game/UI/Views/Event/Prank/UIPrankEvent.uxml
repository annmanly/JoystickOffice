<?xml version="1.0" encoding="utf-8"?>
<UXML
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="UnityEngine.UIElements"
        xmlns:hr="Highrise.UI"
        xmlns:editor="UnityEditor.UIElements"
        xsi:noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd"
>

    <hr:UIPrankEvent class="prank-event">
        <VisualElement name="_prankTouchBlocker"/>
        <VisualElement class="prank-event-content">
            <VisualElement name="_prankingContainer">
                <hr:UILabel name="_stageLabel"/>
                <hr:UIEventTickets name="_tickets"/>
                <hr:UILabel key="choose_an_item_to_prank_with" name="_chooseItemLabel"/>
                <VisualElement name="_itemsContainer"/>
            </VisualElement>
            <VisualElement name="_resultsContainer">
                <VisualElement name="_resultTop">
                    <hr:UIImage name="_resultImage"/>
                    <hr:UILabel name="_resultTitle"/>
                </VisualElement>

                <VisualElement name="_resultBottom">
                    <hr:UILabel name="_stageUpdate"/>

                    <hr:UIEventTicketAndRankResults name="_results"/>

                    <hr:UILabel key="tap_to_continue" name="_tapToContinueLabel"/>

                </VisualElement>
            </VisualElement>
            <hr:UISafeAreaBottom/>
        </VisualElement>
    </hr:UIPrankEvent>

</UXML>
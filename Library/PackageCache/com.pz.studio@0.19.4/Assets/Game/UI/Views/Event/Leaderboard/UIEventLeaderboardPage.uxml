<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIEventLeaderboardPage class="event-leaderboard-page">
    <hr:UICollection>
      <hr:UICollectionSection>
        <hr:UICollectionCells name="_hintCells" layout="stack" layout-spacing="0"/>
      </hr:UICollectionSection>
      <hr:UICollectionSection class="event-leaderboard-page__leaderboard-section">
        <hr:UICollectionCells name="_leaderboardCells" layout="stack" layout-spacing="0" layout-estimated-size="60"/>
      </hr:UICollectionSection>
    </hr:UICollection>

    <VisualElement class="event-leaderboard-page__scroll-up-container">
      <hr:UIImage name="_scrollUpButton" class="event-leaderboard-page__scroll-up-button icon-back"/>
    </VisualElement>

  </hr:UIEventLeaderboardPage>

</UXML>
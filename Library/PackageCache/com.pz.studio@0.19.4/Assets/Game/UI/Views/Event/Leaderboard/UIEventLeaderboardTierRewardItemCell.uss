.event-leaderboard-tier-reward-item-cell {
}

.event-leaderboard-tier-reward-item-cell__card {
    margin-top: 12px;
    background-color: var(--color-fishbase-catfish);
    border-radius: 12px 12px 0 0;
}

.event-leaderboard-tier-reward-item-cell__card-collapsed {
    border-bottom-left-radius: 12px;
    border-bottom-right-radius: 12px;
}

.event-leaderboard-tier-reward-item-cell__rank-container {
    flex-direction: row;
    padding: 16px 12px;
    align-items: center;
}

.event-leaderboard-tier-reward-item-cell__rank-icon {
    width: 18px;
    height: 18px;
}

.event-leaderboard-tier-reward-item-cell__rank-label {
    color: var(--color-fishbase-bonito);
    font-size: 17px;
    -unity-font-definition: var(--font-bold);
    margin-left: 8px;
}

.event-leaderboard-tier-reward-item-cell__grid {
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;
    margin: -4px 8px 8px;
}

.event-leaderboard-tier-reward-item-cell__grid UIItemCard {
    width: 22.25%; /* adds up to 90% minus 1% round-off error margin */
    margin: 4px 1.25%; /* adds up to 10% horizontal margin */
}

.event-leaderboard-tier-reward-item-cell__grid UIItemCard #_quantity {
    background-color: var(--color-fishbase-tuna);
    color: var(--color-fishbase-mackerel);
}

.event-leaderboard-tier-reward-item-cell .separator {
    height: 1px;
    background-color: var(--color-fishbase-tuna);
}

.event-leaderboard-tier-reward-item-cell__rank-dropdown {
    flex-direction: row;
    height: 48px;
}

.event-leaderboard-tier-reward-item-cell__rank-dropdown UILabel {
    color: var(--color-fishbase-bonito);
    font-size: 17px;
    -unity-font-definition: var(--font-bold);
    margin: auto;
    margin-left: 12px;
}

.event-leaderboard-tier-reward-item-cell__collapse-button {
    width: 24px;
    height: 24px;
    padding: 4px;
    --unity-image-tint-color: var(--color-fishbase-anchovy);
    margin: auto 12px auto auto;
}

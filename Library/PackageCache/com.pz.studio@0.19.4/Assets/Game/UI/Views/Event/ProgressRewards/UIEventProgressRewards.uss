.event-progress-rewards {
    background-color: var(--color-fishbase-blackfish);
    flex-shrink: 0;
}

.event-progress-rewards #_hint {
    margin-top: 12px;
    margin-bottom: 36px;
    margin-left: 16px;
    margin-right: 16px;
}

.event-progress-rewards #_nextRewardContainer {
    margin: 16px;
}

.event-progress-rewards #_rewardsCells {
    padding-bottom: 140px;
}

.event-progress-rewards .hint .hint-container {
    background-color: var(--color-fishbase-eel);
}

.event-progress-rewards .hint .hint-icon-wrapper {
    background-color: var(--color-fishbase-blackfish);
}

.event-progress-rewards .hint .hint-text {
    color: var(--color-fishbase-anchovy);
}
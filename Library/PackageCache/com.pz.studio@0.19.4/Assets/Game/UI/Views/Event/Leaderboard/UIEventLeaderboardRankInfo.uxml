<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIEventLeaderboardRankInfo class="event-leaderboard-rank-info">
    <VisualElement class="event-leaderboard-rank-info__card">
      <VisualElement class="event-leaderboard-rank-info__overview">
        <hr:UIUserThumbnail name="_userThumbnail" class="event-leaderboard-rank-info__user-thumbnail"/>
        <VisualElement class="event-leaderboard-rank-info__content">
          <VisualElement class="event-leaderboard-rank-info__rank-container">
            <hr:UIImage name="_rankIcon" class="event-leaderboard-rank-info__rank-icon disabled-rank-icon"/>
            <hr:UILabel name="_rankLabel" class="event-leaderboard-rank-info__rank-label"/>
          </VisualElement>
          <VisualElement class="event-leaderboard-rank-info__tickets-container">
            <hr:UIImage name="_ticketsIcon" class="event-leaderboard-rank-info__tickets-icon event-tickets-icon"/>
            <hr:UILabel name="_ticketsLabel" class="event-leaderboard-rank-info__tickets-label"/>
          </VisualElement>
        </VisualElement>
      </VisualElement>
      <VisualElement name="_nextTierContainer" class="event-leaderboard-rank-info__next-tier-container">
        <VisualElement class="event-leaderboard-rank-info__divider"/>
        <VisualElement class="event-leaderboard-rank-info__next-tier-info-container">
          <hr:UIImage name="_nextTierIcon" class="event-leaderboard-rank-info__next-tier-icon"/>
          <hr:UILabel name="_nextTierLabel" class="event-leaderboard-rank-info__next-tier-label"/>
        </VisualElement>  
      </VisualElement>
    </VisualElement>
  </hr:UIEventLeaderboardRankInfo>

</UXML>
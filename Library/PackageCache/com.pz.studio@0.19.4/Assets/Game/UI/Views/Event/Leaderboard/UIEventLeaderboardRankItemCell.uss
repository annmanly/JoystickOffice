.event-leaderboard-rank-item-cell {
    background-color: var(--color-fishbase-catfish);
}

.event-leaderboard-rank-item-cell__card {
    flex-direction: row;
    padding: 12px;
    align-items: center;
}

.event-leaderboard-rank-item-cell__card-first {
}

.event-leaderboard-rank-item-cell__card-last {
    border-bottom-left-radius: 12px;
    border-bottom-right-radius: 12px;
}

.event-leaderboard-rank-item-cell__card-local {
    background-color: var(--color-fishbase-tuna);
}

.event-leaderboard-rank-item-cell__rank {
    min-width: 32px;
    color: var(--color-fishbase-bonito);
    font-size: 15px;
    -unity-text-align: middle-center;
}

.event-leaderboard-rank-item-cell__thumbnail {
    margin-left: 8px;
    flex-shrink: 0;
}

.event-leaderboard-rank-item-cell .user-thumbnail-user {
    width: 44px;
    height: 44px;
}

.event-leaderboard-rank-item-cell .user-thumbnail-user .user-thumbnail__image-container {
    background-color: var(--color-fishbase-eel);
}

.event-leaderboard-rank-item-cell__name {
    color: var(--color-fishbase-bonito);
    font-size: 15px;
    -unity-font-definition: var(--font-bold);
    margin-left: 8px;
    margin-right: 8px;
    text-overflow: ellipsis;
    -unity-text-overflow-position: end;
    overflow: hidden;
    white-space: nowrap;
    flex-shrink: 1;
}

.event-leaderboard-rank-item-cell__tickets-container {
    flex-direction: row;
    margin-left: auto;
    flex-shrink: 0;
}

.event-leaderboard-rank-item-cell__tickets-icon {
    width: 18px;
    height: 18px;
}

.event-leaderboard-rank-item-cell__tickets-label {
    color: var(--color-fishbase-bonito);
    font-size: 15px;
    margin-left: 4px;
}

.event-leaderboard-rank-item-cell #_divider {
    background-color: var(--color-fishbase-catfish);
    height: 1px;
}

.event-leaderboard-rank-item-cell .separator {
    height: 1px;
    margin-left: 52px;
    background-color: var(--color-fishbase-tuna);
}

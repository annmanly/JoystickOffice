<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:hr="Highrise.UI"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
>

  <hr:UIMyItems class="my-items" focusable="true">
    
    <VisualElement class="my-items__pages">
      <hr:UIPageView name="_pageView" interactable="false"/>
      <hr:UIButton name="_addToGrabButton" class="button-primary overlay-bottom">
        <hr:UILabel name="_addToGrabLabel"/>
      </hr:UIButton>
    </VisualElement>
    
   <hr:UIScrollView name="_tabs" mode="Horizontal" class="my-items__bottom-bar"> 
     
     <VisualElement name="_clothingButton" class="my-items__bottom-bar-cell">
       <hr:UIImage class="icon-inventory-clothing">
         <VisualElement class="my-items__bottom-bar-cell-new-badge"/>
       </hr:UIImage>
       <hr:UILabel key="closet"/>
     </VisualElement>

     <VisualElement name="_miscButton" class="my-items__bottom-bar-cell">
       <hr:UIImage class="icon-inventory-misc">
         <VisualElement class="my-items__bottom-bar-cell-new-badge"/>
       </hr:UIImage>
       <hr:UILabel key="consumable"/>
     </VisualElement>

     <VisualElement name="_emotesButton" class="my-items__bottom-bar-cell">
       <hr:UIImage class="icon-inventory-emotes">
         <VisualElement class="my-items__bottom-bar-cell-new-badge"/>
       </hr:UIImage>
       <hr:UILabel key="emote"/>
     </VisualElement>

     <VisualElement name="_petsButton" class="my-items__bottom-bar-cell">
       <hr:UIImage class="icon-inventory-pets">
         <VisualElement class="my-items__bottom-bar-cell-new-badge"/>
       </hr:UIImage>
       <hr:UILabel key="pet"/>
     </VisualElement>

     <VisualElement name="_backgroundsButton" class="my-items__bottom-bar-cell">
       <hr:UIImage class="icon-inventory-background">
         <VisualElement class="my-items__bottom-bar-cell-new-badge"/>
       </hr:UIImage>
       <hr:UILabel key="background"/>
     </VisualElement>

     <VisualElement name="_furnitureButton" class="my-items__bottom-bar-cell">
       <hr:UIImage class="icon-inventory-furniture">
         <VisualElement class="my-items__bottom-bar-cell-new-badge"/>
       </hr:UIImage>
       <hr:UILabel key="furniture"/>
     </VisualElement>

     <VisualElement name="_nftButton" class="my-items__bottom-bar-cell">
       <hr:UIImage class="icon-inventory-nft">
         <VisualElement class="my-items__bottom-bar-cell-new-badge"/>
       </hr:UIImage>
       <hr:UILabel key="closet_category_nft"/>
     </VisualElement>
     
   </hr:UIScrollView>
    
  </hr:UIMyItems>

</UXML>
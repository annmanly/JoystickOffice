.rarity-none .rarity-text,
.rarity-common .rarity-text { color: var(--color-rarities-light-text); }
.rarity-uncommon .rarity-text { color: var(--color-rarities-light-text); }
.rarity-rare .rarity-text { color: var(--color-rarities-light-text); }
.rarity-epic .rarity-text { color: var(--color-rarities-dark-text); }
.rarity-legendary .rarity-text { color: var(--color-rarities-dark-text); }
.rarity-mythical .rarity-text { color: var(--color-rarities-mythical-gray); }

.rarity-none .rarity-effects-icon,
.rarity-common .rarity-effects-icon { --unity-image-tint-color: var(--color-rarities-light-text); }
.rarity-uncommon .rarity-effects-icon { --unity-image-tint-color: var(--color-rarities-light-text); }
.rarity-rare .rarity-effects-icon { --unity-image-tint-color: var(--color-rarities-light-text); }
.rarity-epic .rarity-effects-icon { --unity-image-tint-color: var(--color-rarities-dark-text); }
.rarity-legendary .rarity-effects-icon { --unity-image-tint-color: var(--color-rarities-dark-text); }
.rarity-mythical .rarity-effects-icon { --unity-image-tint-color: var(--color-rarities-mythical-gray); }

.alt.rarity-none .rarity-text,
.alt.rarity-common .rarity-text { color: var(--color-rarities-common-dark); }
.alt.rarity-uncommon .rarity-text { color: var(--color-rarities-uncommon-dark); }
.alt.rarity-rare .rarity-text { color: var(--color-rarities-rare-dark); }
.alt.rarity-epic .rarity-text { color: var(--color-rarities-epic); }
.alt.rarity-legendary .rarity-text { color: var(--color-rarities-legendary); }
.alt.rarity-mythical .rarity-text { color: var(--color-rarities-mythical-gray); }

.rarity-none.rarity-border,
.rarity-none .rarity-border,
.rarity-common.rarity-border,
.rarity-common .rarity-border { border-color: var(--color-rarities-common-dark); }
.rarity-uncommon.rarity-border,
.rarity-uncommon .rarity-border { border-color: var(--color-rarities-uncommon-dark); }
.rarity-rare.rarity-border,
.rarity-rare .rarity-border { border-color: var(--color-rarities-rare-dark); }
.rarity-epic.rarity-border,
.rarity-epic .rarity-border { border-color: var(--color-rarities-epic); }
.rarity-legendary.rarity-border, 
.rarity-legendary .rarity-border { border-color: var(--color-rarities-legendary); }
.rarity-mythical.rarity-border,
.rarity-mythical .rarity-border { border-color: var(--color-rarities-mythical); }


.rotate-90 { rotate: 90deg; }
.rotate-180 { rotate: 180deg; }
.rotate-270 { rotate: 270deg; }

.hidden { display: none; }

.popup-darken { background-color: var(--color-fishbase-blackfish-60); }


/*

    View loading

*/

.view-hide-until-loaded {
    opacity: 0;
}

.view-loaded.view-hide-until-loaded,
.view-loaded .view-hide-until-loaded {
    opacity: 1;
    transition-property: opacity;
    transition-duration: 0.1s;
    transition-timing-function: ease-out;
}

/**

    Pull Tab

*/

.pull-tab-vertical {
    margin-left: auto;
    margin-right: auto;
    width: 58px;
    height: 6px;
    background-color: var(--color-fishbase-bonito);
    border-radius: 2px;
    opacity: 0.5;
}

/*

    RadioButtonGroup

*/

.radio-button-group {
    background-color: var(--color-fishbase-catfish);
    border-radius: 12px;
    padding: 0;
    padding-left: 12px;
    margin: 0;
}

.radio-button-group-item {
    align-items: center;
    padding: 0;
    padding-top: 12px;
    padding-bottom: 12px;
    border-radius: initial;
    border-bottom-width: 1px;
    border-bottom-color: var(--color-fishbase-tuna);
    margin-left: 0px;
    margin-right: 0px;
}

.radio-button-group-item:checked {
    border-radius: initial;
}

.radio-button-group-item .unity-radio-button__text {
    color: var(--color-fishbase-mackerel);
    padding: 0;
    margin: 0;
}

.radio-button-group-item:checked .unity-radio-button__text {
    color: var(--color-fishbase-whitefish);
}

.radio-button-group-item .radio-button-group-item-indicator {
    display: none;
    flex-shrink: 0;
    width: 16px;
    height: 16px;
    margin-right: 12px;
}

.radio-button-group-item:checked .radio-button-group-item-indicator {
    --unity-image-tint-color: var(--color-accents-secondary);
    display: flex;
}

.radio-button-group-item .unity-radio-button__checkmark-background {
    display: none;
}

.radio-button-group-last-item {
    border-bottom-width: 0;
}

UIKeyboardContainer {
    background-color: var(--color-fishbase-eel);
}

.button {
    -unity-font-definition: var(--font-bold);
}

/*** Narrow Landscape View ***/
.landscape .landscape-narrow {
    position: initial;
    min-width: 375px;
    width: 50%;
    flex-grow: 1;
    align-self: center;
}

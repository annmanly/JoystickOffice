/**

Overlay

*/

.overlay {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}

.overlay-top {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
}

.overlay-top-safe {
    position: absolute;
    top: var(--safe-area-top);
    left: 0;
    right: 0;
}


.overlay-bottom {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
}

.overlay-bottom-safe {
    position: absolute;
    left: 0;
    right: 0;
    bottom: var(--safe-area-bottom);
}

.overlay-right {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
}

.overlay-left { position: absolute; top: 0; left: 0; bottom: 0; }
.overlay-top-right { position: absolute; top: 0; right: 0; }
.overlay-top-right-safe { position: absolute; top: var(--safe-area-top); right: var(--safe-area-right); }
.overlay-top-left { position: absolute; top: 0; left: 0; }
.overlay-top-left-safe { position: absolute; top: var(--safe-area-top); left: var(--safe-area-left); }

.overlay-bottom-right {
    position: absolute;
    bottom: 0;
    right: 0;
}

.overlay-bottom-left {
    position: absolute;
    bottom: 0;
    left: 0;
}

.overlay-safe {
    position: absolute;
    top: var(--safe-area-top);
    left: var(--safe-area-left);
    right: var(--safe-area-right);
    bottom: var(--safe-area-bottom);
}

